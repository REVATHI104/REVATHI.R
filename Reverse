public class Reverse {

	public static void main(String[] args) {
		String str=args[0];
		char[] ch=str.toCharArray();
		
		for(int i=0;i<ch.length;i++)
		{
			for(int j=i+1;j<ch.length;j++)
			{
			if(ch[i]==ch[j])
			{
				ch[j]='$';
			}
			//System.out.println(ch[i]);
		}
			}
		for(int i=0;i<ch.length;i++)
		{
			if(ch[i]!='$')
			{
		System.out.println(ch[i]);
	}}
		
	}
	
	
	
}
